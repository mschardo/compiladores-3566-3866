000 DIM A (10)
010 LET N = 10
020 FOR I=0 TO N-1
030 READ A(I)
040 NEXT I
050 DATA 2.0, 2.5, 1.8, 1.0, 0.8, 3.1, 3.5, 2.9, 2.7, 1.3 060 FOR I=N-1 TO 0 STEP -1
070 FOR J=0 TO I-1
080 IF A(J) > A(J+1) THEN 100
090 GOTO 130
100 LET X = A(J)
110 LET A(J) = A(J+1)
120 LET A(J+1) = X
130 NEXT J
140 NEXT I
150 PRINT A